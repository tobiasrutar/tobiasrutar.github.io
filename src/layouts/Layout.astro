---
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta
			name="description"
			content="Personal blog of Tobias Walter Rutar, a computer science student"
		/>
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
		<meta name="google-site-verification" content="BYV06UGONYAGGFsQ018NlJD_zY_S5sxEgWFzhUtEvT0" />
	</head>
	<body>
		<slot />
	</body>
</html>
